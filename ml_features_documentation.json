{
  "features": [
    {
        "name":"problem_embedding",
        "source":"problem",
        "type":"vector","null":"zero_vector"
    },
    {
        "name":"solution_embedding",
        "source":"solution",
        "type":"vector","null":"zero_vector"
    },
    {
        "name":"market_size_usd",
        "source":"market.market_size_usd",
        "type":"numeric","null":"0"
    },
    {
        "name":"market_growth_rate",
        "source":"market.growth_rate",
        "type":"numeric","null":"0"
    },
    {
        "name":"has_tam_sam_som",
        "source":"market.has_TAM_SAM_SOM",
        "type":"binary","null":"0"
    },
    {
        "name":"team_total_years",
        "source":"team.total_years_parsed",
        "type":"numeric","null":"0"
    },
    {
        "name":"mrr_usd",
        "source":"traction.mrr_usd",
        "type":"numeric","null":"0"
    },
    {
        "name":"pilot_cost_reduction_pct",
        "source":"traction.pilot_cost_reduction_pct",
        "type":"numeric","null":"0"
    },
    {
        "name":"user_retention_pct",
        "source":"traction.user_retention_pct",
        "type":"numeric","null":"0"
    },
    {
        "name":"biz_model_onehot",
        "source":"business_model.type",
        "type":"onehot",
        "values":["subscription","transaction","ads","licensing"],
        "null":"unknown"
    },
    {
        "name":"gtm_channels_count",
        "source":"go_to_market.gtm_channels_count",
        "type":"numeric","null":"0"
    },
    {
        "name":"extraction_confidence_overall",
        "source":"extraction_confidence.*",
        "type":"numeric_mean","null":"0"
    },
    {
        "name":"completeness_ratio",
        "source":"completeness_ratio",
        "type":"numeric","null":"0"
    },
    {
        "name":"desired_ticket_usd",
        "source":"matching_prefs.desired_ticket_usd",
        "type":"numeric","null":"0"
    },
    {
        "name":"desired_stage",
        "source":"matching_prefs.desired_stage",
        "type":"categorical",
        "values":["pre-seed","seed","series_a","series_b","growth"],
        "null":"unknown"
    },
    {
        "name":"industry_tags",
        "source":"matching_prefs.industry_tags",
        "type":"set","null":"[]"
    },
    {
        "name":"industry_tags_embedding",
        "source":"matching_prefs.industry_tags",
        "type":"vector",
        "null":"zero_vector"
    },
    {
        "name":"startup_embedding",
        "source":"concatenate(problem,solution,market,industry_tags)",
        "type":"vector","null":"zero_vector"
    }
  ],

  "targets": [
    {"name":"score_problem","range":"0-5"},
    {"name":"score_solution","range":"0-5"},
    {"name":"score_market","range":"0-5"},
    {"name":"score_team","range":"0-5"},
    {"name":"investment_readiness","range":"0-5"}
  ]
}
